package  {	import starling.core.Starling;	import flash.display.Sprite;	import flash.display.StageAlign;	import flash.display.StageScaleMode;		/**	 * @author @BuiltByEdgar	 */	public class StarlingKnob extends Sprite 	{		private var mStarling:Starling;				public function StarlingKnob() 		{			stage.scaleMode = StageScaleMode.NO_SCALE;			stage.align = StageAlign.TOP_LEFT;			  			mStarling = new Starling(Knob, stage);			mStarling.antiAliasing = 2;			mStarling.start();						var stats:Stats = new Stats();			this.addChild(stats);					}	}}